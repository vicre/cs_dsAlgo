
      procedure PrintOut( N : integer );	{ Print Non-Negative N }
      begin
          if N < 0 then
              Error( 'n is negative' )
          else
          if N < 10 then
              PrintDigit( N )
          else
          begin
              PrintOut( N div 10 );
              PrintDigit( N mod 10 );
          end;
      end;


